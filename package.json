{
  "name": "setaria-ui-pro",
  "version": "0.0.1",
  "description": "开箱即用的中台前端解决方案",
  "author": "UME-Team Ray",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy:page": "vue-cli-service build --dest example && gh-pages -d example && rm -rf example"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "node-sass": "^4.8.3",
    "numeral": "^2.0.6",
    "sass-loader": "^7.0.1",
    "setaria": "^0.2.3",
    "setaria-ui": "^1.3.3",
    "throttle-debounce": "^1.0.1",
    "vue": "^2.5.13",
    "vue-echarts": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-airbnb": "^3.0.0-beta.6",
    "gh-pages": "^1.1.0",
    "vue-template-compiler": "^2.5.13"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/extensions": [
        "error",
        "always",
        {
          "js": "never",
          "vue": "never"
        }
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
